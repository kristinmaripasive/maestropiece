<p id="notice"><%= notice %></p>

<h1>Collabs</h1>
  <div class="list-container">
    <% @collabs.each do |collab| %>
    
      <div class="list-header-container">
        <div id="wrapper"><div id="im"><img src='<%= collab.photo_url %>'/></div></div>
        <div id="name"><%= collab.title %></div>
        <div id="description"><%= collab.description %></div>
        <%= link_to 'Show', collab %>
        <%= link_to 'Edit', edit_collab_path(collab) %>
        <%= link_to 'Destroy', collab, method: :delete, data: { confirm: 'Are you sure?' } %>
      </div>

    <% end %>
  </div>


<br>

<%= link_to 'New Collab', new_collab_path %>
